{
  "name": "tbx-live-server",
  "displayName": "TBX Live Server",
  "description": "Professional live development server with integrated webview preview, multi-server management, and instant reload capabilities for modern web development in VS Code.",
  "version": "6.0.6",
  "publisher": "tbx",
  "author": {
    "name": "Tyler Mailman (TBX Development Team)",
    "email": "themailmaninbox@gmail.com",
    "url": "https://github.com/TheMailmans"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "live server",
    "live preview",
    "live reload",
    "webview",
    "browser preview",
    "development server",
    "html preview",
    "mobile testing",
    "debugging",
    "chrome debugging",
    "side-by-side",
    "integrated preview",
    "web development",
    "frontend",
    "responsive design",
    "cross-browser",
    "multi-root workspace",
    "typescript",
    "javascript",
    "css",
    "html"
  ],
  "preview": false,
  "galleryBanner": {
    "color": "#007ACC",
    "theme": "dark"
  },
  "sponsor": {
    "url": "https://buymeacoffee.com/th3mailman"
  },
  "funding": {
    "type": "buymeacoffee",
    "url": "https://buymeacoffee.com/th3mailman"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension-enhanced.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.liveServer.goOffline",
        "title": "Stop TBX Live Server",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.goOnline",
        "title": "Open with TBX Live Server",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.showFileBrowser",
        "title": "Show File Browser",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.changeWorkspace",
        "title": "Change TBX Live Server Workspace",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.startWebview",
        "title": "Start Webview",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.navigateHome",
        "title": "Navigate Home",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.zoomIn",
        "title": "Zoom In",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.zoomOut",
        "title": "Zoom Out",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.resetZoom",
        "title": "Reset Zoom",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.toggleSplitView",
        "title": "Toggle Split View",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.toggleFullScreen",
        "title": "Toggle Full Screen",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.openDevTools",
        "title": "Open Developer Tools",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.inspectElement",
        "title": "Inspect Element",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.viewSource",
        "title": "View Source",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.clearHistory",
        "title": "Clear Navigation History",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.saveState",
        "title": "Save Navigation State",
        "category": "TBX Live Server"
      },
      {
        "command": "extension.liveServer.loadState",
        "title": "Load Navigation State",
        "category": "TBX Live Server"
      }
    ],
    "keybindings": [
      {
        "command": "extension.liveServer.goOffline",
        "key": "alt+L alt+C",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+C"
      },
      {
        "command": "extension.liveServer.goOnline",
        "key": "alt+L alt+O",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+O"
      },
      {
        "command": "extension.liveServer.startWebview",
        "key": "alt+L alt+W",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+W"
      },
      {
        "command": "extension.liveServer.navigateHome",
        "key": "alt+L alt+H",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+H"
      },
      {
        "command": "extension.liveServer.zoomIn",
        "key": "alt+L alt+=",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+="
      },
      {
        "command": "extension.liveServer.zoomOut",
        "key": "alt+L alt+-",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+-"
      },
      {
        "command": "extension.liveServer.resetZoom",
        "key": "alt+L alt+0",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+0"
      },
      {
        "command": "extension.liveServer.toggleFullScreen",
        "key": "alt+L alt+F",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+F"
      },
      {
        "command": "extension.liveServer.openDevTools",
        "key": "alt+L alt+D",
        "when": "editorTextFocus",
        "mac": "cmd+L cmd+D"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.liveServer.goOnline",
          "group": "myGrp@tbxLivePreview",
          "when": "resourceLangId == html"
        },
        {
          "command": "extension.liveServer.goOffline",
          "group": "myGrp@tbxLivePreview",
          "when": "resourceLangId == html"
        },
        {
          "command": "extension.liveServer.goOnline",
          "group": "myGrp@tbxLivePreview",
          "when": "resourceLangId == xml"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.liveServer.goOnline",
          "group": "navigation@-TBXLivePreview",
          "when": "resourceLangId == html"
        },
        {
          "command": "extension.liveServer.goOnline",
          "group": "navigation@-TBXLivePreview",
          "when": "resourceLangId == xml"
        }
      ]
    },
    "configuration": [
      {
        "title": "Live Server Config",
        "properties": {
          "liveServer.settings.port": {
            "type": [
              "number"
            ],
            "default": 5500,
            "minimum": 0,
            "maximum": 65535,
            "description": "Set Custom Port Number of Live Server. Set 0 if you want random port."
          },
          "liveServer.settings.root": {
            "type": [
              "string"
            ],
            "default": "/",
            "pattern": "/|/[^\\/]",
            "description": "Set Custom root of Live Server. \nTo change root the the server to sub folder of workspace, use '/' and relative path from workspace. \nExample: /subfolder1/subfolder2"
          },
          "liveServer.settings.CustomBrowser": {
            "type": [
              "string",
              "null"
            ],
            "default": null,
            "enum": [
              "chrome",
              "chrome:PrivateMode",
              "firefox",
              "firefox:PrivateMode",
              "microsoft-edge",
              "blisk",
              null
            ],
            "description": "Specify custom browser settings for Live Server. \nBy Default it will open your default favorite browser."
          },
          "liveServer.settings.ChromeDebuggingAttachment": {
            "type": [
              "boolean"
            ],
            "default": false,
            "description": "Enable Chrome Debugging Attachment to Live Server at Debuging Port 9222.\n NOTE: You have to install 'Debugger for Chrome' \nIf the value is true, Select 'Attach to Chrome' from Debug Window to start debugging. \n\n CAUTION: If it is true, 'Launch Chrome against localhost' may not work."
          },
          "liveServer.settings.AdvanceCustomBrowserCmdLine": {
            "type": [
              "string",
              "null"
            ],
            "default": null,
            "description": "Note: If it is not Null, It will override CustomBrowser and ChromeDebuggingAttachment settings.\n\n Examples : \nchrome --incognito --headless --remote-debugging-port=9222 \n C:\\\\Program Files\\\\Firefox Developer Edition\\\\firefox.exe --private-window"
          },
          "liveServer.settings.showOnStatusbar": {
            "type": "boolean",
            "default": true,
            "description": "Change this to false if you don't want the button to show in the statusbar"
          },
          "liveServer.settings.NoBrowser": {
            "type": "boolean",
            "default": false,
            "description": "If it is true live server will start without browser opened."
          },
          "liveServer.settings.ignoreFiles": {
            "type": "array",
            "default": [
              ".vscode/**",
              "**/*.scss",
              "**/*.sass",
              "**/*.ts"
            ],
            "description": "To ignore specific file changes"
          },
          "liveServer.settings.mount": {
            "type": "array",
            "items": {
              "type": "array",
              "minItems": 2,
              "maxItems": 2,
              "items": {
                "type": "string"
              }
            },
            "default": [],
            "description": "Mount a directory to a route. Such as [['/components', './node_modules']]"
          },
          "liveServer.settings.donotShowInfoMsg": {
            "type": "boolean",
            "default": false,
            "description": "To disable information pop up messages."
          },
          "liveServer.settings.donotVerifyTags": {
            "type": "boolean",
            "default": false,
            "description": "To turn off prompt warning message if body or head or other supporting tag is missing in your HTML."
          },
          "liveServer.settings.host": {
            "type": "string",
            "default": "127.0.0.1",
            "description": "To switch between localhost or 127.0.0.1 or anything else. Default is 127.0.0.1"
          },
          "liveServer.settings.useLocalIp": {
            "type": "boolean",
            "default": false,
            "description": "Use local IP as host"
          },
          "liveServer.settings.proxy": {
            "type": "object",
            "default": {
              "enable": false,
              "baseUri": "/",
              "proxyUri": "http://127.0.0.1:80"
            },
            "properties": {
              "enable": {
                "type": "boolean",
                "default": false,
                "description": "Make it true to enable the feature."
              },
              "baseUri": {
                "type": "string",
                "default": "/",
                "pattern": ""
              },
              "proxyUri": {
                "type": "string",
                "default": "http://127.0.0.1:80",
                "pattern": "(^http[s]?://)(.[^(\\|\\s)]+)$"
              }
            },
            "required": [
              "enable",
              "baseUri",
              "proxyUri"
            ],
            "additionalProperties": false,
            "description": "To Setup Proxy"
          },
          "liveServer.settings.useWebExt": {
            "type": "boolean",
            "default": false,
            "description": "You have to install a browser extension. That will be works for your dynamic pages (like PHP)."
          },
          "liveServer.settings.wait": {
            "type": "number",
            "default": 100,
            "description": "Delay before live reloading. Value in milliseconds. Default is 100"
          },
          "liveServer.settings.fullReload": {
            "type": "boolean",
            "default": false,
            "description": "By Default Live Server inject CSS changes without full reloading of browser. You can change this behavior by making this setting as `true`"
          },
          "liveServer.settings.https": {
            "type": "object",
            "default": {
              "enable": false,
              "cert": "",
              "key": "",
              "passphrase": ""
            },
            "properties": {
              "enable": {
                "type": "boolean",
                "default": false,
                "description": "Make it true to enable it."
              },
              "cert": {
                "type": "string",
                "default": "",
                "description": "Add full path certificate."
              },
              "key": {
                "type": "string",
                "default": "",
                "description": "Add full path key"
              },
              "passphrase": {
                "type": "string",
                "default": "",
                "description": "Add Pass Phrase."
              }
            },
            "required": [
              "enable",
              "cert",
              "key",
              "passphrase"
            ],
            "additionalProperties": false,
            "description": "Setup https configuration"
          },
          "liveServer.settings.file": {
            "type": "string",
            "default": "",
            "description": "When set, serve this file (server root relative) for every 404 (useful for single-page applications)"
          },
          "liveServer.settings.multiRootWorkspaceName": {
            "type": "string",
            "default": null,
            "description": "This the entry point of server when you're in multiroot workspace"
          }
        }
      },
      {
        "title": "TBX Live Server Config",
        "properties": {
          "tbxLivePreview.webviewPanel.defaultView": {
            "type": "string",
            "enum": [
              "split",
              "tab",
              "window"
            ],
            "default": "tab",
            "description": "Default view mode for the webview panel"
          },
          "tbxLivePreview.webviewPanel.initialSize": {
            "type": "object",
            "properties": {
              "width": {
                "type": "number",
                "minimum": 400,
                "maximum": 1920,
                "default": 800
              },
              "height": {
                "type": "number",
                "minimum": 300,
                "maximum": 1080,
                "default": 600
              }
            },
            "required": [
              "width",
              "height"
            ],
            "additionalProperties": false,
            "description": "Initial size of the webview panel"
          },
          "tbxLivePreview.webviewPanel.retainContextWhenHidden": {
            "type": "boolean",
            "default": true,
            "description": "Retain webview context when panel is hidden"
          },
          "tbxLivePreview.webviewPanel.autoShowOnStartup": {
            "type": "boolean",
            "default": false,
            "description": "Automatically show webview panel on startup"
          },
          "tbxLivePreview.webviewPanel.preserveState": {
            "type": "boolean",
            "default": true,
            "description": "Preserve webview state across sessions"
          },
          "tbxLivePreview.server.syncWithLiveServer": {
            "type": "boolean",
            "default": true,
            "description": "Synchronize with Live Server when available"
          },
          "tbxLivePreview.server.independentPort": {
            "type": "boolean",
            "default": false,
            "description": "Use independent port instead of Live Server port"
          },
          "tbxLivePreview.server.customPort": {
            "type": "number",
            "default": 5501,
            "minimum": 0,
            "maximum": 65535,
            "description": "Custom port for Inline Live Server server"
          },
          "tbxLivePreview.ui.theme": {
            "type": "string",
            "enum": [
              "auto",
              "light",
              "dark"
            ],
            "default": "auto",
            "description": "Theme for the webview interface"
          },
          "tbxLivePreview.ui.showToolbar": {
            "type": "boolean",
            "default": true,
            "description": "Show toolbar in webview panel"
          },
          "tbxLivePreview.ui.showAddressBar": {
            "type": "boolean",
            "default": true,
            "description": "Show address bar in webview panel"
          },
          "tbxLivePreview.ui.showStatusBar": {
            "type": "boolean",
            "default": true,
            "description": "Show status bar in webview panel"
          },
          "tbxLivePreview.ui.compactMode": {
            "type": "boolean",
            "default": false,
            "description": "Enable compact mode for UI elements"
          },
          "tbxLivePreview.ui.customCss": {
            "type": "string",
            "default": "",
            "description": "Custom CSS for webview styling"
          },
          "tbxLivePreview.navigation.historySize": {
            "type": "number",
            "default": 50,
            "minimum": 10,
            "maximum": 500,
            "description": "Maximum navigation history entries"
          },
          "tbxLivePreview.navigation.enableBackForward": {
            "type": "boolean",
            "default": true,
            "description": "Enable back/forward navigation buttons"
          },
          "tbxLivePreview.navigation.enableKeyboardShortcuts": {
            "type": "boolean",
            "default": true,
            "description": "Enable keyboard shortcuts for navigation"
          },
          "tbxLivePreview.navigation.autoRefreshOnFileChange": {
            "type": "boolean",
            "default": true,
            "description": "Automatically refresh on file changes"
          },
          "tbxLivePreview.navigation.syncWithEditor": {
            "type": "boolean",
            "default": true,
            "description": "Synchronize navigation with editor cursor position"
          },
          "tbxLivePreview.navigation.contextMenu": {
            "type": "boolean",
            "default": true,
            "description": "Enable context menu in webview"
          },
          "tbxLivePreview.development.debugMode": {
            "type": "boolean",
            "default": false,
            "description": "Enable debug mode for development"
          },
          "tbxLivePreview.development.showDevToolsOnStart": {
            "type": "boolean",
            "default": false,
            "description": "Show developer tools on startup"
          },
          "tbxLivePreview.development.logLevel": {
            "type": "string",
            "enum": [
              "error",
              "warn",
              "info",
              "debug"
            ],
            "default": "info",
            "description": "Logging level for development"
          },
          "tbxLivePreview.development.enableExperimentalFeatures": {
            "type": "boolean",
            "default": false,
            "description": "Enable experimental features"
          },
          "tbxLivePreview.development.customUserAgent": {
            "type": "string",
            "default": "",
            "description": "Custom user agent string"
          },
          "tbxLivePreview.development.disableCache": {
            "type": "boolean",
            "default": false,
            "description": "Disable caching for development"
          },
          "tbxLivePreview.advanced.maxConcurrentPanels": {
            "type": "number",
            "default": 3,
            "minimum": 1,
            "maximum": 10,
            "description": "Maximum number of concurrent panels"
          },
          "tbxLivePreview.advanced.panelTimeout": {
            "type": "number",
            "default": 300000,
            "minimum": 60000,
            "maximum": 3600000,
            "description": "Panel timeout in milliseconds"
          },
          "tbxLivePreview.advanced.memoryLimit": {
            "type": "number",
            "default": 100,
            "minimum": 50,
            "maximum": 500,
            "description": "Memory limit in MB"
          },
          "tbxLivePreview.advanced.enableServiceWorker": {
            "type": "boolean",
            "default": false,
            "description": "Enable service worker for offline support"
          },
          "tbxLivePreview.advanced.customHeaders": {
            "type": "object",
            "patternProperties": {
              "^.*$": {
                "type": "string"
              }
            },
            "additionalProperties": false,
            "description": "Custom HTTP headers for requests"
          }
        }
      }
    ]
  },
