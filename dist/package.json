  "scripts": {
    "vscode:prepublish": "npm run prepare:live-server && webpack --mode production",
    "compile": "webpack --mode development",
    "compile:prod": "webpack --mode production",
    "compile:dev": "webpack --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:analyze": "webpack --mode production --env analyze",
    "watch": "webpack --mode development --watch",
    "test": "npm run test:playwright",
    "test:playwright": "playwright test",
    "test:playwright:install": "playwright install",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:run": "npx ts-node test/playwright/test-runner.ts",
    "test:playwright:full": "npm run test:playwright:run",
    "test:screenshots": "npx playwright test screenshot-generation.test.ts --headed --project=screenshot-generation",
    "precommit": "npm run build",
    "prepush": "npm run test",
    "clean": "rimraf dist out",
    "prepare:live-server": "node ./scripts/ensure-live-server-deps.js",
    "postinstall": "npm run prepare:live-server",
    "prepublishOnly": "node scripts/pre-publish-checks.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  },
